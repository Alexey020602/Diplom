
@{
    
    var header = "Какой-то заголовок";
    var menuItems = new List<string>
    {
        "Первый элемент",
        "Второй элемент",
        "Третий элемент"
    };
}
@functions {

    void PrintList(List<string> list)
    {
        <ul>
            @foreach (var element in list)
            {
                <li>@element</li>
            }
        </ul>
    }
